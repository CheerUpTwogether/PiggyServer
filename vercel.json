{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],

  "routes": [
    {
      "src": "/sendNotification",
      "dest": "/api/sendNotification.js"
    },
    { "src": "/mapHtml", "dest": "/public/map.html" }
  ]
}
